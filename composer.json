{
    "name": "rw4lll/ical",
    "description": "ICalendar parser, based on u01jmg3/ics-parser",
    "homepage": "https://github.com/rw4lll/iCal",
    "keywords": [
        "ical",
        "ical-parser",
        "icalendar",
        "ics",
        "ics-parser"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Shitikov",
            "role": "Developer/Owner"
        }
    ],
    "require": {
        "php": ">=7.4 || ~8.0.0 || ~8.1.0",
        "ext-mbstring": "*",
        "symfony/string": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "rector/rector": "^0.13.7",
        "vimeo/psalm": "^4.24"
    },
    "autoload": {
        "psr-4": {
            "Rw4lll\\ICal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
